<div class="lists-page-container" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(<%= image_path "background_image.jpg" %>);">

  <% @lists.each do |list| %>
    <%= link_to(new_list_item_list_path(list)) do %>
      <div class="project-card-container">
        <div class="project-card-header">
          <%= image_tag "kitchen.jpg", class: "project-card-image"%>
          <!-- link_to("X", list_path(list), method: :delete, data: {confirm: "Are you sure you want to delete"}) %> -->
        </div>
        <div class="project-card-body">
          <div>
            <p class="project-card-body-name"><%= list.project_name %></p>
            <p class="project-card-body-count">22</p>
          </div>
          <div>
            <p class="project-card-body-date-created">9/5/20</p>
          </div>
        </div>
      </div>
    <% end %>
  <% end %>
  <%= link_to "+" ,new_list_path %>
  <!-- <div class="project-card-container-blank">

      </div> -->
</div>
